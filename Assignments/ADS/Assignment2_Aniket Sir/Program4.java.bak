import java.util.Scanner;
class Program4
{
	public static void main(String args[])
	{
		String str2= "";
		Scanner sc= new Scanner(System.in);
		
		String str1= sc.nextLine();
		
		for(int i=str1.length()-1; i>=0; i--)
		{
			char ch= str1.charAt(i);
			str2= str2+ch;
		}
		System.out.println(str2);
	}
	
			
}